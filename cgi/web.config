<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <remove name="cgi" />
            <add name="cgi" path="*.asp" verb="*" modules="CgiModule" scriptProcessor="C:\Python27\python.exe %s %s" resourceType="Unspecified" />
        </handlers>
    </system.webServer>
</configuration>
